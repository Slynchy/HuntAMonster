{
  "name": "mh-decisions-decisions",
  "version": "1.0.0",
  "description": "Selects a Monster Hunter quest based on parameters",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && nyc ./node_modules/.bin/_mocha 'test/**/*.test.ts'",
    "start": "ts-node src/index.ts",
    "build": "npm run lint && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "buildJSON": "npx ./scripts/generateQuestJSON.js",
    "buildMonsterJSON": "npx ./scripts/generateMonsterJSON.js",
    "lint": "npx tslint --project tsconfig.json -c tslint.json 'src/**/*.ts'"
  },
  "keywords": [
    "monster-hunter",
    "monster-hunter-rise",
    "mh",
    "mhrise"
  ],
  "author": "Sam Lynch",
  "license": "MIT",
  "devDependencies": {
    "@testdeck/mocha": "^0.1.2",
    "@types/js-search": "^1.4.0",
    "chai": "^4.3.4",
    "html2json": "^1.0.2",
    "js-search": "^2.0.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  }
}
